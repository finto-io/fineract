        <!-- Add card_fields table -->
        <createTable tableName="card_fields">
            <column name="savings_account_id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column defaultValueComputed="NULL" name="card_id" type="VARCHAR(256)"/>
            <column defaultValueComputed="NULL" name="card_bin" type="VARCHAR(6)"/>
            <column defaultValueComputed="NULL" name="card_pan" type="VARCHAR(4)"/>
            <column defaultValueComputed="NULL" name="card_holder_name" type="VARCHAR(256)"/>
            <column defaultValueComputed="NULL" name="external_source" type="BOOLEAN"/>
        </createTable>

